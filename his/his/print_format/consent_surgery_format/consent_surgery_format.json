 "html": "<style>\r\nhtml {\r\n    font-size: 100%;\r\n    box-sizing: border-box\r\n}\r\n\r\n*,\r\n*::before,\r\n*::after {\r\n    box-sizing: inherit\r\n}\r\n\r\nbody {\r\n    padding: 0;\r\n    margin: 0;\r\n    font-family: \"Public Sans\", sans-serif;\r\n    font-size: .875rem;\r\n    font-weight: 300;\r\n    color: #000\r\n}\r\n\r\ntable, th, td {\r\n  border: 1px solid black;\r\n  border-collapse: collapse;\r\n}\r\nh1,\r\nh2,\r\nh3 {\r\n    font-weight: 300;\r\n    color: #000;\r\n    margin-top: 0\r\n}\r\n\r\np {\r\n    margin-top: 0\r\n}\r\n\r\na,\r\na:visited,\r\na:hover {\r\n    text-decoration: none\r\n}\r\n\r\n.marginX {\r\n    margin-inline: 50px\r\n}\r\n\r\n.header {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    -webkit-margin-before: 19px;\r\n    margin-block-start: 19px;\r\n    justify-content: center;\r\n    padding-bottom: 8px;\r\n    /*border-bottom: 1px solid #000*/\r\n}\r\n\r\n.header__top {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 9px\r\n}\r\n\r\n.header__logo {\r\n    width: 70px;\r\n    height: 70px;\r\n    -o-object-fit: contain;\r\n    object-fit: contain;\r\n    padding-bottom: 9px\r\n}\r\n\r\n.header__title {\r\n    font-weight: bold;\r\n    text-transform: uppercase\r\n}\r\n\r\n.header__midd {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 39px\r\n}\r\n\r\n.header__phone {\r\n    font-size: 1.125rem\r\n}\r\n\r\n.header__bottom {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 3px;\r\n    margin-left: 59px\r\n}\r\n\r\n.header__gmail {\r\n    font-size: 1.125rem\r\n}\r\n\r\n.header__country {\r\n    font-size: 1.125rem;\r\n    text-align: center\r\n}\r\n\r\n.banner {\r\n    display: flex;\r\n    flex-direction: column;\r\n    -webkit-margin-before: 10px;\r\n    margin-block-start: 10px;\r\n    gap: 8px\r\n}\r\n\r\n.banner__title {\r\n    text-align: center;\r\n    font-weight: bold;\r\n    font-size: 1.0rem;\r\n    text-transform: uppercase\r\n}\r\n\r\n.banner__top {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between\r\n}\r\n\r\n.banner__left {\r\n    border: 1px solid #000\r\n}\r\n\r\n.banner__no {\r\n    text-transform: uppercase;\r\n    font-weight: bold;\r\n    padding-left: 8px\r\n}\r\n\r\n.banner__id {\r\n    text-transform: uppercase;\r\n    border-left: 1px solid #000;\r\n    padding-left: 8px;\r\n    padding-right: 16px;\r\n    font-weight: bold\r\n}\r\n\r\n.banner__right {\r\n    font-weight: bold\r\n}\r\n\r\n.banner__date {\r\n    font-weight: bold\r\n}\r\n\r\n.banner__midd {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 18px\r\n}\r\n\r\n.banner__client {\r\n    font-weight: 400;\r\n    line-height: 1.8\r\n}\r\n\r\n.banner__clientname {\r\n    font-weight: bold;\r\n    text-transform: uppercase\r\n}\r\n\r\n.banner__bottom {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 3px\r\n}\r\n\r\n.banner__contact {\r\n    font-weight: 400\r\n}\r\n\r\n.banner__country {\r\n    font-weight: 400\r\n}\r\n\r\n.table {\r\n    border-collapse: collapse;\r\n    border-top: 1px solid #000;\r\n    border-left: 1px solid #000;\r\n    border-right: 1px solid #000;\r\n    -webkit-margin-before: 15px;\r\n    margin-block-start: 15px\r\n}\r\n\r\n.table__tb {\r\n    width: 100%\r\n}\r\n\r\n.table__tb thead tr th {\r\n    border-left: 1px solid #000 !important;\r\n    text-align: left\r\n}\r\n.table__tb thead tr th:last-child {\r\n    text-align: right\r\n}\r\n\r\n.table__tb thead tr {\r\n     border-bottom: 1px solid #000 !important;\r\n}\r\n\r\n.table__tb tbody tr td {\r\n    text-align: left;\r\n    border-bottom: 1px solid #000\r\n}\r\n\r\n.table__tb tbody tr td:not(:first-child) {\r\n    border-left: 1px solid #000\r\n}\r\n\r\n.table__tb tbody tr td:last-child {\r\n    text-align: right\r\n}\r\n\r\n.footer {\r\n    display: flex;\r\n    flex-direction: column;\r\n    -webkit-margin-before: 30px;\r\n    margin-block-start: 30px;\r\n    color: #000;\r\n    gap: 46px\r\n}\r\n\r\n.footer__note {\r\n    font-weight: 500\r\n}\r\n\r\n.footer__thanks {\r\n    font-weight: 500\r\n}\r\n\r\n.footer__bottom {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between\r\n}\r\n\r\n.footer__left {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 15px\r\n}\r\n\r\n.footer__authSig {\r\n    font-weight: bold;\r\n    text-transform: uppercase\r\n}\r\n\r\n\r\n\r\n/*.footer__line {*/\r\n/*    background: #000;*/\r\n/*    height: 1px;*/\r\n/*    width: 100%*/\r\n/*}*/\r\n\r\n\r\n\r\n\r\n/*# sourceMappingURL=style.css.map */\r\n</style>   \r\n    <header class=\"header marginX\">\r\n          {% if letter_head  -%}\r\n    <div class=\"letter-head text-center\">{{ letter_head }}</div>\r\n    {% endif %}\r\n\r\n    </header>\r\n\r\n    <section class=\"banner marginX\" >\r\n        \r\n        <h5>Patient ID : {{doc.patient}}</h5>\r\n       <h5>Date : {{doc.date}}</h5>\r\n      <h3 class=\"banner__title\">Consent form</h3>\r\n        <h3 class=\"banner__title\" style=\"margin-top: -15px\">Warqadda Ogolaanshaha Qalliinka</h3>\r\n        \r\n   \r\n\r\n      <!--<div class=\"banner__midd\">-->\r\n      <!--  <h4 class=\"banner__client\">Patient:</h4>-->\r\n      <!--  {% if doc.patient_name %}-->\r\n      <!--  <h4 class=\"banner__clientname\">{{ doc.patient_name }}</h4>-->\r\n      \r\n      <!--  <h4 class=\"banner__client\">Age:</h4>-->\r\n      <!--   <h4 class=\"banner__clientname\">{{ frappe.db.get_value(\"Patient\",doc.patient,\"p_age\")}}</h4>-->\r\n      <!--  <h4 class=\"banner__client\">Sex:</h4>-->\r\n      <!--   <h4 class=\"banner__clientname\">{{ frappe.db.get_value(\"Patient\",doc.patient,\"sex\")}}</h4>-->\r\n      <!--  {% endif %}-->\r\n      <!--</div>-->\r\n      \r\n        <div class=\"banner__midd\" style=\"margin-top: -22px\">\r\n        <h4 class=\"banner__client\" style=\"line-hieght: 1.5\">Aniga oo ah qofka magaciisa iyo saxiixiisa ay hoos ku xusan yihiin,  waxa aan caddaynayaa in aan ogolahay in dhakhatiirta  iyo suuxiyayaasha ku fuliyaan qalliinka noociisu yahay  <b> {{doc.surgery_type or \"\"}} </b></h4>\r\n        \r\n      \r\n      </div>\r\n    <div class=\"banner__midd\" style=\"margin-top: -25px\">\r\n        <h4 class=\"banner__client\"> Bukaanka<b> {{doc.patient_name or \"\"}} </b> oo da`diisu/da`deedu tahay <b>{{doc.age}} {{frappe.db.get_value(\"Patient\",doc.patient, \"age_type\")}}</b></h4>\r\n        \r\n      \r\n      </div>\r\n      \r\n      \r\n         <div class=\"banner__midd\" style=\"margin-top: -25px\">\r\n        <h4 class=\"banner__client\"> Dhakhaatiirtu waxey si buuxda iigu sharaxeen qaabka qallinku uu u dhacayo iyo wixii khatar ah oo ka imaan kara ,waxaan oggolahay in qaliin kale iyo waxkasta oo ay dhaqaatiirtu muhiim u aragto lagu sameyn karo buukaanka hadii daruuro keento inta qaliinku uu socdo .waan ku qancay, raalina waan ka ahay qaliinka iyo suxdintaba .\r\nHaddaba waxa aan halkaan si buuxda uga caddaynayaa in aan aqriyey fahmayna waraaqda oggolaanshaha aniga oo aan la i qasbin oo miir qabana  u ogolaaday inay dhakhaatiirta  iyo suuxiyayaasha  qalliinkaas fuliyaan una fasaxnyihiin ,\r\n sameeyaanna wixii ay u arkaan iney faa'iido u tahay nolosha bukaankaas, kalana kulmaan inta ay ku jiraan saalada qalliinka. Waxaan ku qanacsanahay dadaalka dhakhaatiirta, loomana heysto wixii ka yimaada qalliinkaas.\r\n</h4>\r\n        \r\n      \r\n      </div>\r\n      <h4>Magacyada Saxiixayaasha</h4>\r\n    <table >\r\n\t<thead>\r\n\t\t<tr>\r\n\t\t\t<th width=\"50%\" style=\"color:black; \">{{ _(\"Magaca\") }}</th>\r\n\t\t\t<th width=\"25%\" style=\"color:black\" >{{ _(\"Xiriirka\") }}</th>\r\n\t\t\t<th width=\"25%\"   style=\"color:black\">{{ _(\"Saxiixa\") }}</th>\r\n\t\t</tr>\r\n\t</thead>\r\n\t<tbody>\r\n\t\t{%- for i in doc.representors -%}\r\n\t\t<tr>\r\n\t\r\n\t\t\t<td >{{ i.represent_name or \"\"}}</td>\r\n\t\t\t<td >{{ i.relation or \"\" }}</td>\r\n\t\t\t<td >{{ i.signature  or \"\" }}</td>\r\n\t\t</tr>\r\n\t\t{%- endfor -%}\r\n\t</tbody>\r\n\t\r\n</table>\r\n    \r\n    \r\n<!--        <table >-->\r\n            \r\n<!--\t<thead>-->\r\n<!--\t    \t<tr>-->\r\n<!--\t\t\t<th width=\"100%\" style=\"color:black; \" class=\"text-center\" colspan=\"3\"><b>{{ _(\"Marqaatiyaasha\") }}</b></th>-->\r\n\t\t\t\r\n<!--\t\t</tr>-->\r\n<!--\t\t<tr>-->\r\n<!--\t\t\t<th width=\"50%\" style=\"color:black; \">{{ _(\"Magaca\") }}</th>-->\r\n<!--\t\t\t<th width=\"25%\" style=\"color:black\">{{ _(\"Xariirka\") }}</th>-->\r\n<!--\t\t\t<th width=\"25%\" c  style=\"color:black\">{{ _(\"Saxiixa\") }}</th>-->\r\n<!--\t\t</tr>-->\r\n<!--\t</thead>-->\r\n<!--\t<tbody>-->\r\n<!--\t\t{%- for i in doc.marqaatiyal -%}-->\r\n<!--\t\t<tr>-->\r\n\t\r\n<!--\t\t\t<td >{{ i.represent_name or \"\"}}</td>-->\r\n<!--\t\t\t<td >{{ i.relation or \"\" }}</td>-->\r\n<!--\t\t\t<td >{{ i.signature  or \"\" }}</td>-->\r\n<!--\t\t</tr>-->\r\n<!--\t\t{%- endfor -%}-->\r\n<!--\t</tbody>-->\r\n\t\r\n<!--</table>-->\r\n  \r\n    </section>\r\n\r\n\r\n    <footer class=\"footer marginX\">\r\n     \r\n  \r\n\r\n      <div class=\"footer__bottom\">\r\n        <div class=\"footer__left\">\r\n          <span class=\"footer__authSig\">Magaca iyo saxiixa dhakhatarka </span>\r\n         \r\n          <div class=\"footer__line\">___________   ___________</div>\r\n        </div>\r\n        \r\n           <div class=\"footer__left\">\r\n          <span class=\"footer__authSig\">magaca suuxiyaha</span>\r\n        \r\n          <div class=\"footer__line\">___________   ___________</div>\r\n          \r\n        </div>\r\n      </div>\r\n    </footer>",
